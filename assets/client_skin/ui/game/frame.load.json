        "dims": {"width": {"Vw": 100.0}, "height": {"Vh": 100.0}},
        "content": {"justify_main": "SpaceEvenly", "justify_cross": "Center"}
    },
    "BgColor": ["$frame_bg_color"],

    "#c: Header section.":0,
    "header": {
        "AbsoluteStyle": {
            "dims": {"width": {"Percent": 100.0}, "height": {"Percent": 10.0}, "left": {"Percent": 0.0}, "top": {"Percent": 0.0}},
        }
    },

    "#c: Scoreboard section.":0,
    "scoreboard": {
        "AbsoluteStyle": {
            "dims": {"width": {"Percent": 100.0}, "height": {"Percent": 55.0}, "left": {"Percent": 0.0}, "top": {"Percent": 10.0}},
        }
    },

    "#c: Clicker section.":0,
    "clicker": {
        "AbsoluteStyle": {
            "dims": {"width": {"Percent": 100.0}, "height": {"Percent": 35.0}, "left": {"Percent": 0.0}, "top": {"Percent": 65.0}},
        }
    },

    "#c: Disconnector section.":0,
    "disconnector": {
        "AbsoluteStyle": {
            "dims": {"width": {"Percent": 10.0}, "height": {"Percent": 5.0}, "left": {"Percent": 3.0}, "top": {"Percent": 90.0}},
